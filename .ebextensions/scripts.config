container_commands:
    01set_config:
        command: "cp /home/ec2-user/files/config.json ./config/config.json"
        leader_only: true
        ignoreErrors: false
    02run_migration:
        command: "./node_modules/sequelize/bin/sequelize --migrate --env production"
        leader_only: true
        ignoreErrors: false
    03update_classes:
        command: "node scripts/update_course_table.js"
        leader_only: true
        ignoreErrors: false
    04success:
        command: "cat win.txt"

